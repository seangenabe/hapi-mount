{
  "name": "hapi-mount",
  "version": "2.1.1",
  "description": "Mount directories containing your routes, etc. into your hapi server",
  "scripts": {
    "compile": "babel src -d dist",
    "compile:test": "babel test/test-src.js -o test/test.js",
    "pretest": "cross-env BABEL_ENV=test npm run compile && npm run compile:test",
    "test": "tap -c test/test.js",
    "test:babel": "babel-node test/test-src.js src/index.js",
    "prepublish": "npm run compile"
  },
  "homepage": "https://github.com/seangenabe/hapi-mount",
  "author": "Sean Genabe <seangenabe@outlook.com>",
  "repository": "seangenabe/hapi-mount",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "dist/index.js",
  "keywords": [
    "hapi",
    "require",
    "route",
    "method"
  ],
  "dependencies": {
    "hoek": "^4.0.0",
    "lodash.camelcase": "^4.1.1",
    "require-glob-array": "^2.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-parameters": "^6.11.3",
    "babel-plugin-transform-strict-mode": "^6.11.3",
    "cross-env": "^3.0.0",
    "hapi": "^15.0.1",
    "tap": "^7.0.0"
  },
  "nyc": {
    "sourceMaps": false,
    "instrument": false
  }
}
